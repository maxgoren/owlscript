def fib(var n) {
    if (n < 2) {
        return n;
    }
    return fib(n-1)+fib(n-2);
}

def fiblin(var curr, var prev, var count, var limit) {
    if (count == limit) {
        return curr;
    }
    return fiblin(curr+prev, curr, count+1, limit);
}

def fibo(var n) {
    return fiblin(0, 1, 0, n);
}

def main() {
    let i := 0;
    while (i <= 10) {
        println i + ": " + fib(i);
        i := i + 1;
    }
    return 0;
}