def inssort(var x) {
    let i := 1; 
    while (i < size(x)) {
        let j := i; let val := x[i];
        while (j > 0 && x[j-1] > val) {
            x[j] := x[j - 1];
            j--;
        }
        x[j] := val;
        i++;
    }
}
def main() {
    let lst := [ 11, 3, 24, 86, 42, 19, 101, 13];
    println lst;
    inssort(lst);
    println lst;
    sort(lst, &(i,j) -> i > j);
    println lst;
}