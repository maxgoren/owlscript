func makeTree(let x) {
    println "one tree, coming in hot.";
    return [x, nil, nil];
}

func key(let x) {
    return x[0];
}

func left(let x) {
    return x[1];
}

func right(let x) {
    return x[2];
}

func preorder(let tree) {
    if (tree != nil) {
        preorder(left(tree));
        print key(tree);
        print " ";
        preorder(right(tree));
    }
}

func insert(let tree, x) {
    if (tree == nil) {
        return makeTree(x);
    }
    print x;
    print " < ";
    print key(tree);
    println "?";
    if (x < key(tree)) {
        println "left.";
        tree[1] := insert(left(tree), x);
    } else {
        println "right.";
        tree[2] := insert(right(tree), x);
    }
    return tree;
}
