def exch(var xs, var l, var r) {
    let tmp := xs[l];
    xs[l] := xs[r];
    xs[r] := tmp;
}
def bubblesort(var xs, var l, var r) {
    let i := l;
    while (i < r) {
        let j := r;
        while (j > i) {
            if (xs[j] < xs[j-1]) {
                exch(xs, j, j-1);
            }
            j--;
        }
        i++;
    }
}
def inssort(var x) {
    let i := 1; 
    while (i < size(x)) {
        let j := i; let val := x[i];
        while (j > 0 && x[j-1] > val) {
            x[j] := x[j - 1];
            j--;
        }
        x[j] := val;
        i++;
    }
}
def main() {
    let lst := [ 11, 3, 24, 86, 42, 19, 101, 13, 22, 47, 63, 19, 81, 5];
    println lst;
    inssort(lst);
    println lst;
    sort(lst, &(i,j) -> i > j);
    println lst;
}